    "build": "gulp resources-to-lib && tsc && webpack",
    "build:prod": "gulp resources-to-lib && tsc && webpack --mode=production",
    "clean": "rimraf node_modules lib build",
    "start": "run-p -c start:*",
    "start:webpack": "webpack serve",
    "start:jupyter": "cd ./../dev/sh && ./kill-jupyter-server.sh || true && ./start-jupyter-server.sh anonymous",
    "start:tsc": "tsc --watch",
    "tech:storybook:jupyter": "run-p build:tsc:watch:*",
    "tech:example:editor:react": "run-p build:tsc:watch:*",
    "build:tsc:watch:tsc": "tsc --watch",
    "build:tsc:watch:res": "gulp resources-to-lib-watch",
    "typedoc": "typedoc ./src",
    "watch": "tsc --watch",
    "launch:jupyterpool": "cd dist && jupyterpool --port 3266 --ServerApp.jpserver_extensions=\"{'jupyterlab': True, 'jupyterpool': True,  'nbclassic': True }\"",
    "kill": "./../dev/sh/kill-jupyter-server.sh || true"
  },
